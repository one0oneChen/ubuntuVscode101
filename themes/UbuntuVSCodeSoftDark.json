			"settings": {
				"foreground": "#FB8441"
			}
		},
		{
			"scope": "invalid",
			"范围":"无效的",
			"settings": {
				"foreground": "#f44747"
			}
		},
		{
			"scope": "markup.underline",
			"范围":"标记.下划线",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "markup.bold",
			"范围":"标记.粗体",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#EE8241"
			}
		},
		{
			"scope": "markup.heading",
			"范围":"标记.头部",
