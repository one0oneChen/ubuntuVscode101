	"tokenColors": [
		{
			"scope": "emphasis",
			"范围":"重点",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"范围":"加粗",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "header",
			"范围":"头部",
			"settings": {
				"foreground": "#000080"
			}
		},
		{
			"scope": "comment", 
			"范围":"注释",
			"settings": {
				"foreground": "#6d6d6d",
				"fontStyle": "italic"
			}
		},
		{
			"范围":"语言, 字符串, 其他",
			"scope": [
				"constant.language",
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#EE8241"
			}
		},
		{
			"范围":"字面量.数值python",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#A3ECDA"
			}
		},
		{
			"scope": "constant.regexp",
			"范围":"字面量.正则表达式?",
			"settings": {
				"foreground": "#646695"
			}
		},
		{
			"scope": "entity.name.tag",
			"范围":"实体.名称.标签",
			"settings": {
				"foreground": "#EE8241"
			}
		},
		{
			"scope": "entity.name.tag.css",
			"范围":"实体.名称.标签.css",
			"settings": {
				"foreground": "#FB8441"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"范围":"实体.其他.属性名",
			"settings": {
				"foreground": "#9cdcfe"
			}
		},
		{
			"范围":"注释",
			"scope": [
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.class.mixin.css",
				"entity.other.attribute-name.id.css",
				"entity.other.attribute-name.parent-selector.css",
				"entity.other.attribute-name.pseudo-class.css",
				"entity.other.attribute-name.pseudo-element.css",
